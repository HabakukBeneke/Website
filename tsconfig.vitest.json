{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "types": [
      "node",
      "jsdom"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@views/*": [
        "./src/views/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ]
    }
  }
}